# Application settings
APP_HOST=0.0.0.0
APP_PORT=8999
APP_ENV=production

# Security settings
JWT_SECRET_KEY=your-very-long-random-secret-key-change-this
ADMIN_PASSWORD=change-this-admin-password
CORS_ORIGINS=http://localhost:8999,https://yourdomain.com

# Database settings
LOCAL_DB_DBNAME=nodemanager_dev
LOCAL_DB_USER=nodemanager_user
LOCAL_DB_PASSWORD=strong-database-password
LOCAL_DB_HOST=192.168.1.90
LOCAL_DB_PORT=5432

# SSH settings
SSH_KEY_PATH=ssh_key/id_rsa
SSH_USER=root

# Scripts location
SCRIPTS_PATH=/app/scripts

# XrayChecker integration
XRAY_CHECKER_HOST=192.168.1.123
XRAY_CHECKER_PORT=2112
XRAY_CHECKER_JSON_PATH=/root/xraychecker/configs
XRAY_CHECKER_SSH_KEY=ssh_key/xray_checker_key
XRAY_CHECKER_CONTAINER_NAME=xraychecker-xray-checker

# Subscription settings
SUBSCRIPTION_URL=https://your-subscription-url.com
SUBSCRIPTION_REFRESH_HOURS=1

# NodeMonitoring integration
NODEMONITORING_HOST=127.0.0.1
NODEMONITORING_PORT=8223

# RabbitMQ settings
RABBITMQ_HOST=192.168.1.104
RABBITMQ_PORT=5672
RABBITMQ_USER=nodemanager
RABBITMQ_PASS=rabbitmq-password
RABBITMQ_VHOST=/nodemanager_dev

# Telegram notifications
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-chat-id

# Cloudflare settings
DNS_TTL=120
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token

# Performance settings
MAX_WORKERS=10
REQUEST_TIMEOUT=30
DATABASE_POOL_SIZE=10

# Monitoring settings
CLEANUP_OLD_EVENTS_DAYS=30
CHECK_INTERVAL_MINUTES=1

# Rate limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60